<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@stylesheet1.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="myyarnbuddy.SceneController">
   <children>
      <VBox alignment="CENTER" layoutX="14.0" layoutY="90.0" prefHeight="400.0" prefWidth="600.0" stylesheets="@stylesheet1.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="title" text="My Yarn Buddy :)" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="startNewBtn" mnemonicParsing="false" onAction="#switchToNewProject" prefHeight="40.0" prefWidth="239.0" text="START NEW PROJECT">
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="wIPBtn" mnemonicParsing="false" onAction="#switchToWIPPage" prefHeight="40.0" prefWidth="239.0" text="WIP PROJECTS">
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="compProjBtn" mnemonicParsing="false" onAction="#switchToCompProjPage" prefHeight="40.0" prefWidth="239.0" text="COMPLETED PROJECTS">
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="quitBtn" mnemonicParsing="false" onAction="#quitBtn" prefHeight="40.0" prefWidth="239.0" text="SAVE &amp; QUIT">
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
